{
  "manifest_version": 3,
  "name": "Chronix",
  "version": "1.0",
  "description": "Chrome + Time tracking",
  "permissions": ["tabs", "storage"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.jsx"
  },
  "action": {
    "default_popup": "index.html"
  }
}
